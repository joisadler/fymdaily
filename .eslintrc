{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "browser": true,
    "mongo": true
  },
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "max-len": [
      "error",
      {
        "ignoreComments": true
      }
    ]
  }
}