{
  "extends": [
    "react-app",
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "react-hooks",
    "prettier"
  ],
  "rules": {
    "no-restricted-globals": 0,
    "no-console": 0,
    "no-unused-vars": "warn",
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "camelcase": 0,
    "max-len": [
      "warn",
      {
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreRegExpLiterals": true
      }
    ],
    "react-hooks/rules-of-hooks": "error",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/label-has-associated-control": ["error", { "assert": "either", "depth": 25 }],
    "prettier/prettier": [
      "error", {
        "singleQuote": true,
        "semi": true
      }
    ]
  }
}